<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <Version>1.0.4</Version>
    <Description>1.0.4 - Removal of the JSON-LD ID decorations in the models
1.0.3 - Initial build</Description>
    <PackageProjectUrl>https://github.com/endeavour-predict/ep_models</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="QRisk3" Version="3.0.0">
      <Aliases>qrisk3</Aliases>
    </PackageReference>
  </ItemGroup>

</Project>
